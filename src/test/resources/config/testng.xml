<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="suite">
	<listeners>
		<listener class-name="com.automation.Generic.Listener" />
	</listeners>

	<test name="RegisterTest">
		<parameter name="browser" value="Firefox"></parameter>
		<parameter name="mode" value=""></parameter>
		
		<parameter name="username" value="sanchit"></parameter>
		<parameter name="password" value="admin"></parameter>
		
		<classes>
			<class name="com.automation.Test.RegisterTest" />
		</classes>
	</test>
	<test name="LoginTest">
		<parameter name="browser" value="Chrome"></parameter>
		<parameter name="mode" value="start-maximized"></parameter>
		<classes>
			<class name="com.automation.Test.LoginTest" />
		</classes>
	</test>
	<test name="SearchTest">
		<parameter name="browser" value="Chrome"></parameter>
		<parameter name="mode" value="incognito,start-maximized"></parameter>
		<classes>
			<class name="com.automation.Test.SearchFlightTest" />
		</classes>
	</test>
	<test name="BookingTest">
		<parameter name="browser" value="Edge"></parameter>
		<parameter name="mode" value="start-maximized"></parameter>
		<classes>
			<class name="com.automation.Test.BookingTest" />
		</classes>
	</test>
	
</suite> 